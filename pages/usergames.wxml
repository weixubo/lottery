                                            <view class="prize-bg-wraper">
                                                <image class="gpc-3-bg-prize " lazyLoad="lazy-load" mode="widthFix" src="{{format.image( item.prizeImages[0] )}}" style="{{$GPCPending$imageStyle}}" wx:if="{{item.prizeImages.length===1}}"></image>
                                                <swiper autoplay="{{$GPCPending$autoplay}}" indicatorDots="{{$GPCPending$indicatorDots}}" interval="{{3000}}" style="{{$GPCPending$imageStyle}}" wx:elif="{{item.prizeImages.length>1}}">
                                                    <swiper-item wx:for="{{item.prizeImages}}" wx:for-item="prizeImage" wx:key="index">
                                                        <image class="gpc-3-bg-prize " lazyLoad="lazy-load" mode="widthFix" src="{{format.image(prizeImage)}}" style="{{$GPCPending$imageStyle}}"></image>
                                                    </swiper-item>
                                                </swiper>
                                                <image class="" mode="widthFix" src="{{item.theme==='funny'?'/images/default-prize-pi@3x.png':'/images/default-prize@3x.png'}}" style="{{$GPCPending$imageStyle}}" wx:else></image>
