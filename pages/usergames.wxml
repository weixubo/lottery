                                            </view>即开即中，综合中奖率 {{item.winnerRate}}%
          </view>
                                        <view class="lottery-conditions" wx:if="{{item.readable_conditions}}">
                                            <view class="label">参与条件：</view>
                                            <view class="list">
                                                <view class="item" wx:for="{{item.readable_conditions}}" wx:key="index">
                                                    <image src="{{format.conditionImage(item.type,item.value)}}" wx:if="{{format.conditionImage(item.type,item.value)}}"></image>
                                                    <text>{{item.value}}</text>
                                                </view>
                                            </view>
                                        </view>
                                    </view>
                                </view>
                                <view bindtouchmove="$GPCPending$touchMove" bindtouchstart="$GPCPending$touchStart" class="gpc type-{{item.previewType}} {{item.isTouchMove&&(item.previewType===1||item.previewType===4||item.previewType===5)?'touch-move-active':''}}" data-index="{{item.id}}" wx:else>
                                    <view class="gpc-1-container" wx:if="{{item.previewType===1}}">
                                        <view bindtap="$GPCPending$click" class="gpc-3" data-wpyclick-a="{{item}}" hoverClass="highlight">
