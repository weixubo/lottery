                                            <view class="redirect-mina-label">赞助商</view>
                                            <view class="sponsor-name">{{item.sponsor.name}}</view>
                                            <image class="mina-icon" src="https://cdn.readhub.cn/mina/lottery/icn-miniapp@3x.png"></image>
                                            <view class="arrow-right"></view>
                                        </navigator>
                                        <view class="gpc-3-padding-bottom" wx:else></view>
                                    </view>
                                    <view class="gpc-2-container" wx:elif="{{item.previewType===4}}">
                                        <view bindtap="$GPCDone$click" class="gpc-2" data-wpyclick-a="{{item}}" hoverClass="highlight">
                                            <text class="gpc-2-prize-name" wx:if="{{item.prizes[0].type===2}}">幸运金：{{item.prizes[0].displayAmount}}元</text>
                                            <text class="gpc-2-prize-name" wx:else>奖品: {{item.prizes[0].name}}</text>
                                            <text class="gpc-2-prize-icon">x</text>
                                            <text class="gpc-2-prize-count">{{item.prizes[0].count}}</text>
                                            <text class="gpc-2-prize-date" wx:if="{{item.endTime}}">{{item.endTime}}</text>
                                            <view class="arrow-right">
                                                <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/arrow@3x.png" style="width:34px;height:18px;"></image>
                                            </view>
                                        </view>
                                        <view bindtap="$GPCDone$hide" class="del" data-index="{{item.id}}" data-wpyhide-a="{{item}}">
                                            <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/delete@3x.png" style="width:54px;height:54px;"></image>
                                        </view>
                                    </view>
                                    <view class="gpc-2-container" wx:elif="{{item.previewType===5}}">
                                        <view bindtap="$GPCDone$click" class="gpc-2" data-wpyclick-a="{{item}}" hoverClass="highlight">
                                            <view class="gpc-2-prize-name gpc-5-prize-name">奖品:
              <text wx:for="{{item.prizes}}" wx:key="index">{{index!==0?'、':''}}{{item.name}}</text>
                                            </view>
                                            <text class="gpc-2-prize-dot {{$GPCDone$isAndroid?'android':'ios'}}">...等</text>
                                            <text class="gpc-2-prize-date" wx:if="{{item.endTime}}">{{item.endTime}}</text>
                                            <view class="arrow-right">
                                                <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/arrow@3x.png" style="width:34px;height:18px;"></image>
                                            </view>
                                        </view>
                                        <view bindtap="$GPCDone$hide" class="del" data-index="{{item.id}}" data-wpyhide-a="{{item}}">
                                            <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/delete@3x.png" style="width:54px;height:54px;"></image>
                                        </view>
                                    </view>
                                    <view class="gpc-6-container" wx:elif="{{item.previewType===6}}">
                                        <view bindtap="$GPCDone$click" class="gpc-6" data-wpyclick-a="{{item}}" hoverClass="highlight">
                                            <image class="prize-image" lazyLoad="lazy-load" mode="aspectFill" src="{{item.prizeImages[0]?format.image( item.prizeImages[0] ):'/images/default-prize@3x.png'}}"></image>
                                            <view class="prize-info">
                                                <view class="name" wx:if="{{item.prizes.length>0}}">
                                                    <text>奖品：</text>
                                                    <text wx:for="{{item.prizes}}" wx:key="index">{{index!==0?'、':''}}{{item.name}} x{{item.count}}</text>
                                                </view>
                                                <view class="draw" wx:if="{{item.draw_type==='user'&&item.minUserCount}}">
                参与者达到 {{item.minUserCount}} 人时自动开奖
              </view>
                                                <view class="draw" wx:elif="{{item.draw_type==='time'&&item.simpleEndTime}}">
                {{item.simpleEndTime}} 自动开奖
              </view>
                                                <view class="draw" wx:elif="{{item.draw_type==='manual'}}">
                本抽奖将由发起人手动开奖
              </view>
                                                <view class="drawt" wx:elif="{{item.draw_type==='chatroom'}}">
                对实时在线的参与者开奖
              </view>
                                                <view class="tags">
                                                    <view class="tag red" wx:if="{{item.sponsor&&item.sponsor.name}}">
                                                        <image src="https://cdn.readhub.cn/mina/lottery/icn-v2@3x.png"></image>
                                                        <text>{{item.sponsor.name}}</text>
                                                    </view>
                                                    <view class="tag" wx:if="{{item.shopPrize}}">
                                                        <text>商城奖品</text>
                                                    </view>
                                                </view>
                                            </view>
                                        </view>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
                <view bindtap="loadMore" class="loadMore" hoverClass="highlight" wx:if="{{hasMoreGames}}">{{loadingText}}</view>
            </view>
            <view class="empty" wx:else>
                <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/icn-empty-default-1@3x.png"></image>
                <view class="desc">{{emptyDesc}}</view>
            </view>
        </view>
        <view animation="{{$actionsheet$animationData}}" bindtap="$actionsheet$hide" class="cover" style="{{$actionsheet$coverStyle}}"></view>
        <view class="gamechoosesheet {{$actionsheet$show?'show':'hide'}} {{$actionsheet$isIphoneX?'iPhoneX':''}}">
            <view bindtap="$actionsheet$createNormal" class="item" hoverClass="hover">
                <view class="item-name">普通抽奖</view>
                <view class="item-desc">可设置按时间、按人数或手动开奖</view>
            </view>
            <view bindtap="$actionsheet$createRoom" class="item {{$actionsheet$isIphoneX?'iPhoneX':''}}" hoverClass="hover">
                <view class="item-name">实时抽奖<image src="https://cdn.readhub.cn/mina/lottery/realtime-icon-small@3x.png"></image>
                </view>
                <view class="item-desc">对实时在线的参与者开奖</view>
            </view>
        </view>
    </Layout>
</view>
<wxs module="format" src="../wxs/format.wxs" />