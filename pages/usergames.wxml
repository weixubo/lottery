                                            </view>
                                            <view class="gpc-3-padding-bottom"></view>
                                        </view>
                                        <view bindtap="$GPCPending$hide" class="del" data-index="{{item.id}}" data-wpyhide-a="{{item}}">
                                            <image mode="aspectFit" src="https://cdn.readhub.cn/mina/lottery/delete@3x.png" style="width:54px;height:54px;"></image>
                                        </view>
                                    </view>
                                    <view bindtap="$GPCPending$click" class="gpc-3" data-wpyclick-a="{{item}}" hoverClass="{{item.previewType===3?'highlight':''}}" wx:elif="{{item.previewType===3||item.previewType===2}}">
