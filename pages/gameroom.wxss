page {
    background-color: #f0f0f0;
    width: 100%;
    height: 100%;
    font-family: "PingFangSC-Regular";
}

page .auth-tip {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.66);
    z-index: 930;
    display: flex;
    align-items: center;
    justify-content: center;
}

page .auth-tip .box {
    border-radius: 4px;
    box-shadow: 0 7.5px 12.5px 0 rgba(0,0,0,0.15);
    background: #fff;
    max-width: 325px;
    margin: 0px 25px;
    height: 375px;
    display: flex;
    flex-direction: column;
    align-items: center;
}

page .auth-tip .box .title {
    font-size: 16px;
    font-weight: bold;
    color: rgba(0,0,0,0.9);
    margin-top: 45px;
}

page .auth-tip .box .tip {
    font-size: 14px;
    color: rgba(0,0,0,0.5);
    margin-top: 18px;
}

page .auth-tip .box image {
    margin-top: 30px;
    width: 303px;
    height: 160px;
}

page .auth-tip .box .btn {
    margin-top: 12px;
    height: 44px;
    width: 80%;
    line-height: 44px;
    text-align: center;
    background-color: #e2453a;
    color: #ffffff;
    font-size: 15px;
    border-radius: 2px;
}

.gameroom .room-detail {
    margin: 0px 0px 30px;
    padding-bottom: 36px;
}

.gameroom .room-detail.show {
    opacity: 1;
}

.gameroom .room-detail.hide {
    opacity: 0;
}

.gameroom .game-detail-preview {
    margin: 0px 12px 0px;
    position: relative;
    z-index: 2;
}

.gameroom .game-detail-preview .game-detail-preview-cover {
    height: 10px;
    width: 100%;
    background: #f0f0f0;
}

.gameroom .game-detail-preview .game-detail-card {
    border-radius: 6px;
    position: relative;
    background: #fff;
    box-shadow: 0 2px 5px -2px rgba(0,0,0,0.1);
    border: 1rpx outset rgba(0,0,0,0.25);
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    overflow: hidden;
}

.gameroom .room-global-message-container {
    margin: 0px 12px;
}

.gameroom .loading {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 80%;
    margin-top: 70%;
}

.gameroom .loading image {
    width: 28px;
    height: 28px;
}

.gameroom .room-prize {
    padding: 12px 16px;
}

.gameroom .room-prize .game-mannal-start {
    border-top: 1rpx solid rgba(0,0,0,0.08);
    padding-top: 10px;
}

.gameroom .room-prize .game-mannal-start view {
    height: 40px;
    line-height: 40px;
    box-sizing: border-box;
    border-radius: 20px;
    background: linear-gradient(to right,#cc5e52,#d67162);
    border: solid 1rpx rgba(0,0,0,0.1);
    font-size: 17px;
    font-weight: bold;
    letter-spacing: .4px;
    text-align: center;
    color: #ffffff;
}

.gameroom .room-prize .game-mannal-start view.highlight {
    background: #aa5146;
}

.gameroom .room-prize .room-prize-desc {
    display: flex;
    margin-bottom: 10px;
}

.gameroom .room-prize .room-prize-desc .room-prize-title {
    flex: 1 1 auto;
    font-size: 19px;
    letter-spacing: .5px;
    color: rgba(0,0,0,0.8);
    align-self: center;
    margin-right: 8px;
}

.gameroom .room-prize .room-prize-desc .room-prize-name {
    word-break: break-all;
    margin-right: 8px;
}

.gameroom .room-prize .room-prize-desc .room-prize-count-icon {
    font-size: 19px;
    font-weight: 500;
    letter-spacing: .5px;
    color: #ce6155;
    position: relative;
    top: -1px;
}

.gameroom .room-prize .room-prize-desc .room-prize-count {
    font-size: 19px;
    font-weight: 500;
    letter-spacing: .5px;
    color: #ce6155;
}

.gameroom .room-prize .room-prize-desc .room-prize-image {
    height: 36px;
    flex: 0 0 auto;
    align-self: flex-start;
}

.gameroom .room-prize .room-prize-owner {
    border-top: 1rpx solid rgba(0,0,0,0.08);
    display: flex;
    padding-top: 8px;
    font-size: 14px;
    font-weight: 300;
    letter-spacing: .3px;
    color: rgba(0,0,0,0.4);
}

.gameroom .room-prize .room-prize-owner .room-prize-owner-label {
    flex: 1 0 auto;
    align-self: center;
}

.gameroom .room-prize .room-prize-owner .room-prize-owner-avatar {
    flex: 0 0 auto;
    align-self: center;
    height: 22px;
    margin-right: 6px;
}

.gameroom .room-prize .room-prize-owner .room-prize-owner-name {
    flex: 0 0 auto;
    align-self: center;
}

.gameroom .room-prize .room-prize-owner .room-prize-owner-online {
    flex: 0 0 auto;
    align-self: center;
    margin-left: 6px;
}

@-webkit-keyframes animateGlobalMessage {
    0% {
        left: 100%;
    }

    100% {
        left: -100%;
    }
}

@keyframes animateGlobalMessage {
    0% {
        left: 100%;
    }

    100% {
        left: -100%;
    }
}

@-webkit-keyframes animateDanmuMessage {
    0% {
        transform: translateX(0);
    }

    100% {
        transform: translateX(-1500px);
    }
}

@keyframes animateDanmuMessage {
    0% {
        transform: translateX(0);
    }

    100% {
        transform: translateX(-1500px);
    }
}

@-webkit-keyframes avatarShow {
    0% {
        -webkit-transform: scale(0);
    }

    80% {
        -webkit-transform: scale(1.2);
    }

    100% {
        -webkit-transform: scale(1);
    }
}

@keyframes avatarShow {
    0% {
        transform: scale(0);
    }

    80% {
        transform: scale(1.2);
    }

    100% {
        transform: scale(1);
    }
}

@-webkit-keyframes avatarHide {
    100% {
        opacity: 0;
    }
}

@keyframes avatarHide {
    100% {
        opacity: 0;
    }
}

@-webkit-keyframes countShow {
    100% {
        opacity: 0;
        transform: scale(1.35);
    }
}

@keyframes countShow {
    100% {
        opacity: 0;
        transform: scale(1.35);
    }
}

@-webkit-keyframes slotMachineShow {
    0% {
        left: 100%;
    }

    100% {
        left: 0%;
    }
}

@keyframes slotMachineShow {
    0% {
        left: 100%;
    }

    100% {
        left: 0%;
    }
}

@-webkit-keyframes animateAvatar {
    0% {
        margin-left: 0px;
        margin-top: 0px;
    }

    25% {
        margin-left: 0px;
        margin-top: -10px;
    }

    50% {
        margin-left: -10px;
        margin-top: -10px;
    }

    75% {
        margin-left: -10px;
        margin-top: 0px;
    }

    100% {
        margin-left: 0px;
        margin-top: 0px;
    }
}

@keyframes animateAvatar {
    0% {
        margin-left: 0px;
        margin-top: 0px;
    }

    25% {
        margin-left: 0px;
        margin-top: -10px;
    }

    50% {
        margin-left: -10px;
        margin-top: -10px;
    }

    75% {
        margin-left: -10px;
        margin-top: 0px;
    }

    100% {
        margin-left: 0px;
        margin-top: 0px;
    }
}

@-webkit-keyframes resultBGShow {
    100% {
        opacity: 1;
    }
}

@keyframes resultBGShow {
    100% {
        opacity: 1;
    }
}

.gameroom .room-message-bar {
    position: relative;
    height: 26px;
    line-height: 26px;
    padding: 8px;
    background-color: #fff6e5;
    font-size: 15px;
    font-weight: bold;
    letter-spacing: 1.1px;
    text-align: center;
    color: #856733;
    overflow: hidden;
    border-radius: 6px;
    box-shadow: 0 2px 5px -2px rgba(0,0,0,0.1);
    border: 1rpx solid #c3b69e;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
}

.gameroom .room-message-bar .room-message-bar-dot {
    position: absolute;
    width: 4px;
    height: 4px;
    background-color: #f0e1c6;
    border: solid 1rpx #c3b69e;
    border-radius: 50%;
}

.gameroom .room-message-bar .room-message-bar-dot-1 {
    left: 4px;
    top: 4px;
}

.gameroom .room-message-bar .room-message-bar-dot-2 {
    right: 4px;
    top: 4px;
}

.gameroom .room-message-bar .room-message-bar-dot-3 {
    left: 4px;
    bottom: 4px;
}

.gameroom .room-message-bar .room-message-bar-dot-4 {
    right: 4px;
    bottom: 4px;
}

.gameroom .room-message-bar .room-global-message {
    height: 2em;
    font-weight: bold;
    line-height: 2em;
    overflow: hidden;
    position: relative;
    text-align: center;
}

.gameroom .room-message-bar .room-global-message .content {
    white-space: nowrap;
}

.gameroom .chat-bar {
    position: fixed;
    left: 0px;
    bottom: 0px;
    background-color: #f7f7f7;
    border-top: 1rpx solid rgba(0,0,0,0.2);
    width: 100%;
    padding: 8px 0px;
    height: 44px;
}

.gameroom .chat-bar.fix {
    padding-bottom: 38px;
}

.gameroom .chat-bar input {
    align-self: center;
    height: 44px;
    box-sizing: border-box;
    border: solid 1rpx #cccccc;
    background-color: #ffffff;
    border-radius: 4px;
    margin-left: 8px;
    margin-right: 8px;
    padding: 11px 14px;
    font-size: 15px;
    letter-spacing: .4px;
    position: absolute;
    left: 0px;
    z-index: 10;
    transition: width linear .3s;
}

.gameroom .chat-bar input.input-placeholder {
    color: rgba(51,51,51,0.3);
}

.gameroom .chat-bar .chat-bar-speck {
    position: absolute;
    right: 0px;
    width: 91px;
    height: 42px;
    line-height: 42px;
    margin-right: 8px;
    align-self: center;
    border-radius: 4px;
    background: linear-gradient(to top,#f2f2f2,#fafafa);
    border: solid 1rpx #cccccc;
    font-size: 15px;
    letter-spacing: .4px;
    color: rgba(0,0,0,0.6);
    opacity: 1;
    transition: opacity linear .2s;
}

.gameroom .chat-bar .chat-bar-speck.show {
    opacity: 1;
}

.gameroom .chat-bar .chat-bar-speck.hide {
    opacity: 0;
}

.gameroom .chat-bar .chat-bar-speck image {
    margin: 0px 5px 0px 14px;
