}

.gameroom .room-content {
    width: 100%;
    position: relative;
}

.gameroom .room-content .room-count {
    font-size: 13px;
    font-weight: bold;
    color: rgba(0,0,0,0.4);
    position: absolute;
    right: 13px;
    bottom: 9px;
}

.gameroom .room-content .connect-status {
    font-size: 16px;
