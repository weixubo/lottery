    color: rgba(0,0,0,0.8);
    position: absolute;
    left: 13px;
    bottom: 9px;
}

.gameroom .room-content .room-users {
    height: 100%;
    position: relative;
    width: 100%;
    overflow: hidden;
}

.gameroom .room-content .room-users image {
    -webkit-backface-visibility: hidden;
    position: absolute;
    border-radius: 50%;
    border: solid 1rpx rgba(0,0,0,0.1);
}

.gameroom .room-content .room-users image.ios {
