.gameroom .i-want-create-game image {
    width: 27px;
    height: 24px;
    align-self: center;
}

.gameroom .game-detail-users {
    margin: 30px 0px 0px;
    display: block;
    position: absolute;
    left: 0px;
    bottom: -113px;
    width: 100%;
    opacity: 0;
    animation: 2.4s .6s animateShow forwards;
}

.gameroom .game-detail-users.animate-0 {
    display: none;
}

.gameroom .game-detail-users .game-detail-users-banner {
    text-align: center;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value {
    font-size: 15px;
    letter-spacing: .3px;
    text-align: center;
    color: #808080;
    flex: 0 0 auto;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value view {
    display: inline-block;
    color: #45749f;
    font-weight: bolder;
    padding: 0px 6px;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value view.highlight {
    color: #45648f;
    background: #ddd;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value button {
    display: inline;
    font-size: 15px;
    background: transparent;
    color: #45749f;
    font-weight: bolder;
    border: none;
    padding: 0px;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value button.button-hover {
    color: #45648f;
    background: #ddd;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-value button:after {
    display: none;
}

.gameroom .game-detail-users .game-detail-users-banner .game-detail-users-banner-line {
    flex: 1 1 auto;
    height: 1px;
    opacity: .06;
    background-color: #000000;
    margin: 0px 14px;
    align-self: center;
}

.gameroom .game-detail-users .game-detail-users-list {
    margin-top: 20px;
    display: flex;
    flex-wrap: wrap;
    align-content: center;
    align-items: center;
    justify-content: center;
}

.gameroom .game-detail-users .game-detail-users-list .game-detail-users-user {
    width: 33px;
    height: 33px;
    flex: 0 0 auto;
    display: flex;
    flex-direction: column;
    margin: 0px 2px 4px;
}

.gameroom .game-detail-users .game-detail-users-list .game-detail-users-user image {
    width: 33px;
    height: 33px;
}