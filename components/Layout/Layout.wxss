@-webkit-keyframes selectCoverShow {
    100% {
        opacity: 1;
    }
}

@keyframes selectCoverShow {
    100% {
        opacity: 1;
    }
}

@-webkit-keyframes rubberBand {
    from {
        transform: scale3d(1,1,1);
    }

    30% {
        transform: scale3d(1.12,.75,1);
    }

    40% {
        transform: scale3d(.67,1.25,1);
    }

    50% {
        transform: scale3d(1.03,.85,1);
    }

    65% {
        transform: scale3d(.85,1.05,1);
    }

    75% {
        transform: scale3d(1.05,.95,1);
    }

    to {
        transform: scale3d(1,1,1);
    }
}

@keyframes rubberBand {
    from {
        transform: scale3d(1,1,1);
    }

    30% {
        transform: scale3d(1.12,.75,1);
    }

    40% {
        transform: scale3d(.67,1.25,1);
    }

    50% {
        transform: scale3d(1.03,.85,1);
    }

    65% {
        transform: scale3d(.85,1.05,1);
    }

    75% {
        transform: scale3d(1.05,.95,1);
    }

    to {
        transform: scale3d(1,1,1);
    }
}

button:before {
    display: none!important;
}

button:after {
    display: none!important;
}

.layout-conponent .navigation-bar {
    width: 100%;
    display: flex;
    background: #fff;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 950;
}

.layout-conponent .navigation-bar .navigation-bar-main {
    height: 44px;
    padding-top: 20px;
    position: relative;
    width: 100%;
    border-bottom: 1rpx solid rgba(0,0,0,0.1);
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-title {
    font-size: 18px;
    color: #000;
    width: 100%;
    height: 44px;
    line-height: 44px;
    position: relative;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-title .navigation-bar-title-content {
    display: flex;
    justify-content: center;
    font-size: 17px;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-title .navigation-bar-title-content .filter-arrow {
    width: 9px;
    height: 6px;
    padding: 16px 16px 16px 7px;
    align-self: center;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-title .navigation-bar-title-content text {
    align-self: center;
}

.layout-conponent .navigation-bar .navigation-bar-main .more-prize {
    width: 85px;
    height: 14px;
    position: absolute;
    bottom: 4px;
    padding: 11px;
    animation-duration: 1.4s;
    animation-name: rubberBand;
    animation-delay: .8s;
    animation-iteration-count: 2;
    z-index: 951;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-back {
    width: 20px;
    height: 20px;
    position: absolute;
    padding: 16px;
    bottom: -2px;
    z-index: 951;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-cancel {
    height: 20px;
    position: absolute;
    padding: 16px;
    bottom: -4px;
    z-index: 951;
    font-size: 15px;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-cancel.highlight {
    opacity: .5;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-account {
    height: 20px;
    position: absolute;
    padding: 16px;
    bottom: -6px;
    z-index: 951;
    font-size: 13px;
    color: #808080;
}

.layout-conponent .navigation-bar .navigation-bar-main .navigation-bar-account.highlight {
    opacity: .5;
}

.layout-conponent .navigation-bar .navigation-bar-main.iPhoneX {
    padding-top: 44px;
}

.layout-conponent .layout-main {
    padding-top: 64px;
}

.layout-conponent .layout-main::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.layout-conponent .layout-main.iPhoneX {
    padding-top: 88px;
}

.layout-conponent .layout-main.fix {
    padding-top: 0;
}

.layout-conponent .get-user-info-button {
    position: fixed;
    width: 100%;
    height: 100%;
    opacity: 0;
    z-index: 940;
    top: 0;
    left: 0;
}