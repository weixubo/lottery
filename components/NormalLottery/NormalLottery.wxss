.gpc-3 {
    position: relative;
    background: #fff;
}

.gpc-3.highlight {
    background: #ddd;
}

.gpc-3 .gpc-3-bg {
    width: 100%;
    position: relative;
    border-top: 1rpx solid #d8d8d8;
}

.gpc-3 .gpc-3-bg .prize-bg-wraper {
    overflow: hidden;
}

.gpc-3 .gpc-3-bg .prize-bg-wraper image {
    width: 100%;
    display: block;
    position: relative;
}

.gpc-3 .gpc-3-bg .prize-bg-wraper .gpc-3-bg-sponsor {
    position: absolute;
    top: 10px;
    right: 10px;
    height: 28px;
    line-height: 28px;
    padding: 0px 11px;
    font-size: 13px;
    color: #ffffff;
    background-color: #a8a8a8;
    opacity: .9;
}

.gpc-3 .gpc-3-bg .prize-image {
    top: -10%;
}

.gpc-3 .gpc-3-game-user-end {
    position: absolute;
    top: 35px;
    left: 45%;
    width: 104px;
    height: 104px;
    text-align: center;
    border-radius: 50%;
    background: rgba(0,0,0,0.8);
    box-shadow: 0 3px 10.5px 0 rgba(0,0,0,0.3);
}

.gpc-3 .gpc-3-game-user-end .circle {
    width: 92px;
    height: 92px;
    line-height: 92px;
    text-align: center;
    position: absolute;
    left: 4px;
    top: 4px;
    border: 2px solid rgba(255,255,255,0.3);
    border-radius: 50%;
    color: rgba(255,255,255,0.8);
    font-size: 15px;
    font-weight: bold;
}

.gpc-3 .gpc-3-conditions {
    display: flex;
    padding: 0px 15px 2px;
    padding-top: 5px;
    align-items: flex-start;
    align-items: center;
}

.gpc-3 .gpc-3-conditions .gpc-3-conditions-label {
    font-size: 13px;
    color: #808080;
    flex: 0 0 auto;
    position: relative;
    top: -3px;
}

.gpc-3 .gpc-3-conditions .gpc-3-conditions-list {
    display: flex;
    flex-wrap: wrap;
    flex: 1 1 auto;
}

.gpc-3 .gpc-3-conditions .gpc-3-conditions-item {
    background: #f0f0f0;
    padding: 0px 10px 0px 8px;
    margin-right: 6px;
    margin-bottom: 6px;
    display: flex;
    align-items: center;
    height: 20px;
}

.gpc-3 .gpc-3-conditions .gpc-3-conditions-item image {
    width: 12px;
    height: 12px;
    margin-right: 3px;
}

.gpc-3 .gpc-3-conditions .gpc-3-conditions-item text {
    font-size: 11px;
    font-weight: bold;
    color: #808080;
}

.gpc-3 .gpc-3-prize-container {
    padding-top: 13px;
    display: flex;
    position: relative;
}

.gpc-3 .gpc-3-prize-container .gpc-3-sponsor {
    flex: 0 0 auto;
    height: 25px;
    line-height: 25px;
    padding: 0px 8px;
    margin: 0px 15px;
    font-size: 13px;
    color: #575757;
    position: relative;
}

.gpc-3 .gpc-3-prize-container .gpc-3-sponsor:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    border: 1px solid #808080;
    box-sizing: border-box;
    width: 200%;
    height: 200%;
    transform: scale(.5);
    transform-origin: left top;
}

.gpc-3 .gpc-3-prize-container .gpc-3-guaranteed {
    flex: 0 0 auto;
}

.gpc-3 .gpc-3-prize-container .gpc-3-guaranteed image {
    width: 69px;
    height: 22px;
    margin: 0px 15px;
}

.gpc-3 .gpc-3-prize-container .gpc-3-approve {
    flex: 0 0 auto;
    width: 70px;
    height: 22px;
    background-color: #429ad1;
    line-height: 22px;
    text-align: center;
    font-size: 12px;
    color: #ffffff;
    margin-right: 15px;
    font-weight: bold;
}

.gpc-3 .gpc-3-prize-container .gpc-3-approve image {
    width: 15px;
    height: 15px;
    margin-right: 5px;
    position: relative;
    bottom: -3px;
}

.gpc-3 .gpc-3-long-guaranteed {
    position: absolute;
    right: 12px;
    bottom: -32px;
}

.gpc-3 .gpc-3-long-guaranteed image {
    width: 104px;
    height: 22px;
}

.gpc-3 .gpc-3-prize {
    flex: 1 1 auto;
    margin: 0px 0px 0px 15px;
    font-size: 17px;
    letter-spacing: .5px;
    display: flex;
    position: relative;
}

.gpc-3 .gpc-3-prize .gpc-3-prizes {
    flex: 1 1 auto;
    position: relative;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-group-icon {
    width: 20px;
    height: 20px;
    margin-left: 6px;
    position: relative;
    bottom: -4px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-name-label {
    width: 58px;
    height: 16px;
    display: inline-block;
    margin-right: 6px;
    position: relative;
    top: 2px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-name {
    color: rgba(0,0,0,0.8);
    margin-right: 5px;
    display: inline;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-icon {
    position: relative;
    top: -1px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-desc {
    flex: 1 1 auto;
    padding-right: 15px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-desc.gpc-3-prize-desc-muti {
    font-size: 16px;
    margin-bottom: 5px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-desc.gpc-3-prize-desc-muti:last-child {
    margin-bottom: 0px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-desc .money-value {
    color: #cc4b3d;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-edit {
    position: absolute;
    right: 0px;
    top: 0px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-state {
    flex: 0 0 76px;
}

.gpc-3 .gpc-3-prize .gpc-3-prize-state .gpc-3-prize-state-tag {
    width: 74px;
    height: 27px;
    line-height: 27px;
    text-align: center;
    border-radius: 13.5px;
    background-color: #fff;
    border: solid 1rpx #c3a428;
    font-size: 14px;
    color: #c3a428;
}

.gpc-3 .gpc-3-desc {
    display: flex;
    margin: 7px 15px 7px 15px;
    align-items: center;
    justify-content: space-between;
    padding-bottom: 1px;
}

.gpc-3 .gpc-3-desc .gpc-3-desc-count {
    margin: 0;
}

.gpc-3 .gpc-3-desc-count {
    display: flex;
    flex: 0 1 auto;
    font-size: 13px;
    letter-spacing: .3px;
    color: #808080;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin: 7px 10px 7px 15px;
}

.gpc-3 .gpc-3-initiator {
    margin: 17px 0px 0px 15px;
    padding-right: 15px;
    border-top: 1rpx solid rgba(0,0,0,0.1);
    display: flex;
    padding-top: 13px;
    padding-bottom: 3px;
    font-size: 15px;
    letter-spacing: .3px;
    color: rgba(0,0,0,0.9);
    align-items: center;
}

.gpc-3 .gpc-3-initiator.highlight {
    opacity: .6;
}

.gpc-3 .gpc-3-initiator image {
    width: 22px;
    height: 22px;
    flex: 0 0 auto;
    border-radius: 50%;
    margin-left: 20px;
}

.gpc-3 .gpc-3-initiator .gpc-3-initiator-desc {
    flex: 1 0 auto;
    align-self: center;
}

.gpc-3 .gpc-3-initiator .gpc-3-initiator-name {
    flex: 0 1 auto;
    margin-left: 9px;
    align-self: center;
}

.gpc-3 .gpc-3-initiator .arrow-right {
    border: solid #c7c7cc;
    border-width: 0 2px 2px 0;
    display: inline-block;
    padding: 3px;
    transform: rotate(-45deg);
    flex: 0 0 auto;
    align-self: center;
    position: relative;
    top: 1px;
    margin-left: 4px;
}

.gpc-3 .gpc-3-initiator form {
    display: block;
    width: 100%;
}

.gpc-3 .gpc-3-initiator .gpc-3-initiator-contact {
    font-size: 17px;
    font-weight: 500;
    letter-spacing: .4px;
    color: #ce6155;
    text-align: center;
    flex: 1 1 auto;
    height: 35px;
    line-height: 35px;
    border: none;
    background: transparent;
}

.gpc-3 .gpc-3-initiator .gpc-3-initiator-contact:after {
    display: none;
}

.gpc-3 .gpc-3-initiator .gpc-3-initiator-contact.highlight {
    color: #e6b0aa;
}

.gpc-3 .gpc-3-share {
    margin: 15px 15px 0px;
    padding: 17px 0px 7px;
    text-align: center;
    border-top: 1rpx solid rgba(0,0,0,0.08);
    font-size: 17px;
    font-weight: 500;
    letter-spacing: .4px;
    color: #ce6155;
}

.gpc-3 .gpc-3-share.highlight {
    color: #e6b0aa;
}

.gpc-3 .indent-split-line {
    margin-left: 15px;
    height: 0px;
    border-top: 1rpx solid rgba(0,0,0,0.1);
}

.gpc-3 .gpc-3-redirect-mina {
    height: 50px;
    display: flex;
    font-size: 15px;
    letter-spacing: .3px;
    color: rgba(0,0,0,0.9);
    align-items: center;
}

.gpc-3 .gpc-3-redirect-mina.highlight {
    background: #ddd;
}

.gpc-3 .gpc-3-redirect-mina image {
    width: 16px;
    height: 16px;
    margin-left: 7px;
}

.gpc-3 .gpc-3-redirect-mina .redirect-mina-label {
    flex: 1 1 auto;
    margin-left: 15px;
}

.gpc-3 .gpc-3-redirect-mina .arrow-right {
    border: solid #c7c7cc;
    border-width: 0 2px 2px 0;
    display: inline-block;
    padding: 4px;
    transform: rotate(-45deg);
    position: relative;
    flex: 0 0 auto;
    margin-left: 6px;
    align-self: center;
    margin-right: 15px;
}

.gpc-3 .gpc-3-padding-bottom {
    width: 100%;
    height: 10px;
}

.gpc-3 .tags {
    display: flex;
    flex: 0 1 auto;
    justify-content: flex-start;
    flex-wrap: nowrap;
}

.gpc-3 .tags .tag {
    height: 20px;
    padding: 0px 8px;
    background-color: #ffffff;
    color: #575757;
    font-weight: bold;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    position: relative;
}

.gpc-3 .tags .tag:before {
    border: 1px solid rgba(0,0,0,0.5);
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(.5);
    pointer-events: none;
    transform-origin: 0 0;
    content: '';
}

.gpc-3 .tags .tag.ml10 {
    margin-left: 10px;
}

.gpc-3 .tags .tag:last-child {
    margin-right: 0px;
}

.gpc-3 .tags .tag text {
    font-size: 11px;
    white-space: nowrap;
}

.gpc-3 .tags .tag.red {
    position: relative;
}

.gpc-3 .tags .tag.red:before {
    border: 1px solid #ad8b8b;
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(.5);
    pointer-events: none;
    transform-origin: 0 0;
    content: '';
}

.gpc-3 .tags .tag.red text {
    color: #8f5b5b;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.gpc-3 .tags .tag image {
    width: 8px;
    height: 8px;
    margin-right: 4px;
    display: inline-block;
}

.gpc-3 .certificate-guide-banner {
    background: rgba(0,0,0,0.66);
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
    left: 0px;
    top: 0px;
    z-index: 99998;
}

.gpc-3 .certificate-inner-card {
    width: 650rpx;
    background: #fff;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%) scale(1);
    z-index: 99999;
    animation: dialog .5s;
    padding-top: 70rpx;
}

.gpc-3 .certificate-inner-card .certificate-guide-banner-close {
    width: 14px;
    height: 14px;
    position: absolute;
    right: 0px;
    top: 0px;
    padding: 15px;
}

.gpc-3 .certificate-inner-card .certificate-guide-inner-icon {
    width: 87rpx;
    height: 87rpx;
    margin: 0 auto;
    display: block;
}

.gpc-3 .certificate-inner-card .certificate-guide-tags {
    width: 650rpx;
    margin-top: 40rpx;
}

.gpc-3 .certificate-inner-card .certificate-guide-tags .certificate-guide-tag {
    width: 50px;
    margin: 0 auto;
}

.gpc-3 .certificate-inner-card .certificate-guide-tags .certificate-guide-tag text {
    color: #d22220;
}

.gpc-3 .certificate-inner-card .certificate-guide-tags .certificate-guide-tag:before {
    border: 1px solid #d22220;
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(.5);
    pointer-events: none;
    transform-origin: 0 0;
    content: '';
}

.gpc-3 .certificate-inner-card .certificate-guide-inner-title {
    font-size: 21px;
    font-weight: bold;
    color: rgba(0,0,0,0.66);
    letter-spacing: .5px;
    text-align: center;
    margin-top: 40rpx;
}

.gpc-3 .certificate-inner-card .certificate-guide-inner-type {
    font-size: 14px;
    letter-spacing: .4px;
    color: rgba(0,0,0,0.5);
    text-align: center;
    margin-top: 36rpx;
    margin-bottom: 70rpx;
}

.gpc-3 .certificate-inner-card .certificate-guide-banner-sub {
    width: 650rpx;
    background: #fafafa;
    padding-bottom: 70rpx;
}

.gpc-3 .certificate-inner-card .certificate-guide-banner-sub text {
    width: 480rpx;
    text-align: left;
    color: rgba(0,0,0,0.5);
    font-size: 13px;
    margin: 0 auto;
    display: block;
    line-height: 26px;
}

.gpc-3 .certificate-inner-card .certificate-guide-banner-sub button {
    width: 480rpx;
    height: 72rpx;
    background-color: #575757;
    margin: 88rpx auto 0;
    font-size: 15px;
    font-style: normal;
    font-stretch: normal;
    font-weight: bold;
    line-height: 72rpx;
    letter-spacing: .4px;
    text-align: left;
    color: #ffffff;
    text-align: center;
    border-radius: 0;
}

.gpc-3 .certificate-inner-card .certificate-guide-banner-sub button.highlight {
    background-color: #2b2b2b;
}

@keyframes dialog {
    from {
        transform: translate(-50%,-50%) scale(.7);
    }

    to {
        transform: translate(-50%,-50%) scale(1);
    }
}